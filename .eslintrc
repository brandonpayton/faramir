{
  "env": {
    "browser": true,
    "node": true
  },

  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "extends": "eslint:recommended",
  "rules": {
    "no-console": [ 0 ],
    "quotes": [ 2, "single", "avoid-escape" ],
    "strict": [ 2, "never" ]
  }
}
